{
  "configurations": [
    {
      "name": "windows-gcc-x86",
      "defines": [
        "${default}",
        "ENV_UNIT_TESTS",
        "PLATFORM_HOSTED",
        "RUN_WITH_PROFILING"
      ],
      "includePath": [
        "${workspaceFolder}/**"
      ],
      "compilerPath": "C:/MinGW/bin/gcc.exe",
      "intelliSenseMode": "windows-gcc-x86",
      "browse": {
        "limitSymbolsToIncludedHeaders": true
      },
      "cStandard": "${default}",
      "cppStandard": "${default}"
    },
    {
      "name": "Sim",
      "defines": [
        "${default}",
        "PLATFORM_HOSTED",
        "RUN_WITH_PROFILING"
      ],
      "includePath": [
        "${default}",
        "qkrt-mcb-project/taproot/sim-modm/hosted-linux/modm/src"
      ],
      "compilerPath": "g++",
      "intelliSenseMode": "gcc-x64",
      "browse": {
        "limitSymbolsToIncludedHeaders": true
      }
    },
    {
      "name": "Hardware",
      "includePath": [
        "${default}",
        "qkrt-mcb-project/taproot/modm/src",
        "qkrt-mcb-project/taproot/modm/ext/cmsis/core",
        "qkrt-mcb-project/taproot/modm/ext/cmsis/device",
        "qkrt-mcb-project/taproot/modm/ext/cmsis/dsp"
      ],
      "compilerPath": "arm-none-eabi-g++",
      "intelliSenseMode": "gcc-arm",
      "browse": {
        "limitSymbolsToIncludedHeaders": true
      }
    }
  ],
  "version": 4
}